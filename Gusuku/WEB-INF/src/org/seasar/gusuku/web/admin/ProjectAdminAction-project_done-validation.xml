<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN"
        "http://www.opensymphony.com/xwork/xwork-validator-1.0.dtd">
<validators>
	<field name="name">
		<field-validator type="requiredstring">
			<message key="project.name.required"/>
		</field-validator>
	</field>
	<field name="key">
		<field-validator type="requiredstring">
			<message key="project.key.required"/>
		</field-validator>
	</field>
	<field name="workflowid">
		<field-validator type="required">
			<message key="project.workflowid.required"/>
		</field-validator>
	</field>
</validators>